<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>

    <bean id="restRoutes" class="de.stekoe.idss.RestRoutes" />
    <bean id="applicationRoutes" class="de.stekoe.idss.ApplicationRoutes" />
    <bean id="wicketApplication" class="de.stekoe.idss.WebApplication" />

    <bean id="projectDataProvider" class="de.stekoe.idss.model.provider.ProjectDataProvider" />
</beans>
